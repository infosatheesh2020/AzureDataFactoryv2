{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "hacker1-satheesh-adf"
		},
		"AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage'"
		},
		"AzureSqlDatabase_cloudsales_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase_cloudsales'"
		},
		"AzureSqlDatabase_cloudstreaming_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase_cloudstreaming'"
		},
		"CosmosDb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosDb'"
		},
		"FourthCoffee_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FourthCoffee'"
		},
		"VanArsdel_SqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'VanArsdel_SqlServer'"
		},
		"DestinationDataset_fj7_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "movies.json"
		},
		"DestinationDataset_fj7_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "movies"
		},
		"AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsg2store.dfs.core.windows.net"
		},
		"FourthCoffee_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "openhackadmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_accountKey')]"
					},
					"AzureSqlDatabase_cloudsales_connectionString": {
						"value": "[parameters('AzureSqlDatabase_cloudsales_connectionString')]"
					},
					"AzureSqlDatabase_cloudstreaming_connectionString": {
						"value": "[parameters('AzureSqlDatabase_cloudstreaming_connectionString')]"
					},
					"CosmosDb_connectionString": {
						"value": "[parameters('CosmosDb_connectionString')]"
					},
					"FourthCoffee_password": {
						"value": "[parameters('FourthCoffee_password')]"
					},
					"VanArsdel_SqlServer_connectionString": {
						"value": "[parameters('VanArsdel_SqlServer_connectionString')]"
					},
					"AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"DestinationDataset_fj7_properties_typeProperties_fileName": {
						"value": "[parameters('DestinationDataset_fj7_properties_typeProperties_fileName')]"
					},
					"DestinationDataset_fj7_properties_typeProperties_folderPath": {
						"value": "[parameters('DestinationDataset_fj7_properties_typeProperties_folderPath')]"
					},
					"FourthCoffee_properties_typeProperties_userId": {
						"value": "[parameters('FourthCoffee_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}